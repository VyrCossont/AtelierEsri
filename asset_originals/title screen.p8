-- animated title screen prototype
function _init()
 a = 1
 a_max = 18
 q = 0
 q_max = 25
end

function _update60()
 if btnp(⬅️) and a > 1 then
  a -= 1
 end
 if btnp(➡️) and a < a_max then
  a += 1
 end
 
 if q == q_max then
  if a == a_max then
   a = 1
  else
   a += 1
  end
  q = 0
 else
  q += 1
 end
end

function _draw()
 cls()
 if (a == 1) return
 
 boring_text("atelier", 35, 40)
 if (a == 2) return
 
 boring_text("esri", 47, 60)
 if (a == 3) return
 
 pal(8, 14)
 pal(2, 14)
 spr_atelier(20, 20)
 pal()
 if (a == 4) return
 
 pal(8, 14)
 spr_atelier(20, 20)
 pal()
 if (a == 5) return
 
 spr_atelier(20, 20)
 if (a == 6) return
 
 pal(9, 15)
 pal(10, 15)
 spr_esri(40, 55)
 pal()
 if (a == 7) return
 
 pal(10, 15)
 spr_esri(40, 55)
 pal()
 if (a == 8) return
 
 spr_esri(40, 55)
 if (a == 9) return
 
 cls()
 spr_atelier(20, 20)
 spr_esri(40, 55)
 if (a == 10) return
 
 print("& the", 58, 87, 6)
 if (a == 11) return
 
 print("\^w\^tastral nonsense", 10, 95, 13)
 print("\^pastral nonsense", 10, 95, 6)
 if (a == 12) return
end

function boring_text(s, x, y)
 print("\^w\^t"..s, x, y, 6)
 print("\^p"..s, x, y, 7)
end


function spr_atelier(x, y)
 spr(1, x, y, 12, 6)
end

function spr_esri(x, y)
 spr(97, x, y, 8, 4)
end
